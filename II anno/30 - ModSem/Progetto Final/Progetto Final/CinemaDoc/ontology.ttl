@prefix : <http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/mario/ontologies/esame/progettoCinema> .

<http://www.semanticweb.org/mario/ontologies/esame/progettoCinema> rdf:type owl:Ontology ;
                                                                    owl:imports <http://www.ontologydesignpatterns.org/cp/owl/bag.owl> .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/language
<http://purl.org/dc/elements/1.1/language> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/type
<http://purl.org/dc/elements/1.1/type> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/knows
<http://xmlns.com/foaf/0.1/knows> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/name
<http://xmlns.com/foaf/0.1/name> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> rdf:type owl:ObjectProperty ;
                                                               rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                                                               rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> ;
                                                               rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item> ;
                                                               rdfs:comment "has item - The link to every item of the Bag" ;
                                                               rdfs:label "has item" .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent> rdf:type owl:ObjectProperty ;
                                                                   rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item> ;
                                                                   rdfs:range [ rdf:type owl:Class ;
                                                                                owl:complementOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item>
                                                                              ] ;
                                                                   rdfs:comment "itemContent - The link to the actual resource to which the item refers." ;
                                                                   rdfs:label "item content" .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf> rdf:type owl:ObjectProperty ;
                                                              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                                                              owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> ;
                                                              rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item> ;
                                                              rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> ;
                                                              rdfs:comment "item of - The link from an item to the Bag where it is contained" ;
                                                              rdfs:label "item of" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#AmbientatoIn
:AmbientatoIn rdf:type owl:ObjectProperty ;
              owl:inverseOf :AmbientazioneDi ;
              rdfs:domain :Film ;
              rdfs:range :Ambientazione .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#AmbientazioneDi
:AmbientazioneDi rdf:type owl:ObjectProperty ;
                 rdfs:domain :Ambientazione ;
                 rdfs:range :Film .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#ContieneCast
:ContieneCast rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Film ;
              rdfs:range :Cast .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#DIrettoDa
:DIrettoDa rdf:type owl:ObjectProperty ;
           owl:inverseOf :Dirige ;
           rdfs:domain :Film ;
           rdfs:range :NazionalitaRegista .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#DirettoreAllaMusicaDi
:DirettoreAllaMusicaDi rdf:type owl:ObjectProperty ;
                       owl:inverseOf :HaDirettore ;
                       rdfs:domain :DirettoreAllaMusica ;
                       rdfs:range :Film .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Dirige
:Dirige rdf:type owl:ObjectProperty ;
        rdfs:domain :NazionalitaRegista ;
        rdfs:range :Film .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#EventoDi
:EventoDi rdf:type owl:ObjectProperty ;
          owl:inverseOf :haEventi ;
          rdfs:domain :Eventi ;
          rdfs:range :Film .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#FilmProdotto
:FilmProdotto rdf:type owl:ObjectProperty ;
              rdfs:domain :OrigineDiProduzione ;
              rdfs:range :Film ;
              owl:propertyChainAxiom ( :ProdottoDa
                                       :Dirige
                                     ) .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#HaAttore
:HaAttore rdf:type owl:ObjectProperty ;
          owl:inverseOf :RecintaIn ;
          rdfs:domain :Film ;
          rdfs:range :Attore .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#HaDirettore
:HaDirettore rdf:type owl:ObjectProperty ;
             rdfs:domain :Film ;
             rdfs:range :DirettoreAllaMusica .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#HaPersonaggio
:HaPersonaggio rdf:type owl:ObjectProperty ;
               owl:inverseOf :PersonaggioDi .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#InterpretaPersonaggio
:InterpretaPersonaggio rdf:type owl:ObjectProperty ;
                       owl:inverseOf :InterpretatoDa ;
                       rdfs:domain :Attore ;
                       rdfs:range :Personaggio .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#InterpretatoDa
:InterpretatoDa rdf:type owl:ObjectProperty ;
                rdfs:domain :Personaggio ;
                rdfs:range :Attore .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#PersonaggioDi
:PersonaggioDi rdf:type owl:ObjectProperty ;
               rdfs:domain :Personaggio ;
               rdfs:range :Film .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#ProdottoDa
:ProdottoDa rdf:type owl:ObjectProperty ;
            owl:inverseOf :ProduceFilm ;
            rdfs:domain :OrigineDiProduzione ;
            rdfs:range :NazionalitaRegista .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#ProduceFilm
:ProduceFilm rdf:type owl:ObjectProperty ;
             rdfs:domain :NazionalitaRegista ;
             rdfs:range :OrigineDiProduzione .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#RecintaIn
:RecintaIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Attore ;
           rdfs:range :Film .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#RegistaDi
:RegistaDi rdf:type owl:ObjectProperty ;
           owl:inverseOf :haRegista ;
           rdfs:domain :Regista ;
           rdfs:range :Film .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#ScrittoreDi
:ScrittoreDi rdf:type owl:ObjectProperty ;
             owl:inverseOf :haScrittore ;
             rdfs:domain :Scrittore ;
             rdfs:range :Film .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#haComeCast
:haComeCast rdf:type owl:ObjectProperty ;
            rdfs:domain :Cast ;
            rdfs:range :Attore .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#haEventi
:haEventi rdf:type owl:ObjectProperty ;
          rdfs:domain :Film ;
          rdfs:range :Eventi .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#haNazionalita
:haNazionalita rdf:type owl:ObjectProperty ;
               rdfs:domain :Regista ;
               rdfs:range :NazionalitaRegista .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#haRegista
:haRegista rdf:type owl:ObjectProperty ;
           rdfs:domain :Film ;
           rdfs:range :Regista .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#haRuolo
:haRuolo rdf:type owl:ObjectProperty ;
         rdfs:domain :Persona ;
         rdfs:range :Ruolo .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#haScrittore
:haScrittore rdf:type owl:ObjectProperty ;
             rdfs:domain :Film ;
             rdfs:range :Scrittore .


#################################################################
#    Data properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#size
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#size> rdf:type owl:DatatypeProperty ;
                                                            rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                            rdfs:range xsd:integer ;
                                                            rdfs:comment "size - The number of items belonging to a collection" ;
                                                            rdfs:label "size" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#AnnoDiUscita
:AnnoDiUscita rdf:type owl:DatatypeProperty ;
              rdfs:domain :Film ;
              rdfs:range xsd:int ;
              rdfs:comment "anno in cui un film è stato rilasciato" ;
              rdfs:label "AnnoDiUscita" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#CasaCinematografica
:CasaCinematografica rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Film ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Casa produttoria di un film" ;
                     rdfs:label "CasaCinematografica" ;
                     rdfs:seeAlso <https://dbpedia.org/page/Production_company> .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#HaNrFilm
:HaNrFilm rdf:type owl:DatatypeProperty ;
          rdfs:domain :Film ;
          rdfs:range xsd:integer ;
          rdfs:label "HaNrFilm" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Nome
:Nome rdf:type owl:DatatypeProperty ;
      rdfs:domain :Film ;
      rdfs:range xsd:string ;
      rdfs:comment "Titolo di un film" ;
      rdfs:label "Nome" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#RankingAttore
:RankingAttore rdf:type owl:DatatypeProperty ;
               rdfs:domain :Attore ;
               rdfs:range xsd:int .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#RankingFilm
:RankingFilm rdf:type owl:DatatypeProperty ;
             rdfs:domain :Film ;
             rdfs:range xsd:int .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Valutazione
:Valutazione rdf:type owl:DatatypeProperty ;
             rdfs:domain :Film ;
             rdfs:range xsd:float ;
             rdfs:comment "Valutazione che un film ha ricevuto dalla critica" ;
             rdfs:label "Valutazione" .


#################################################################
#    Classes
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> rdf:type owl:Class ;
                                                           rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                           rdfs:comment "Bag - Collection that can have a number of copies of each object" ;
                                                           rdfs:label "(collections) Bag" .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item> rdf:type owl:Class ;
                                                            rdfs:subClassOf owl:Thing ,
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent> ;
                                                                              owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                            ] ;
                                                            owl:disjointWith <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                            rdfs:comment "Item - Element belonging to a Bag" ;
                                                            rdfs:label "item" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#AcclamatiDallaCritica
:AcclamatiDallaCritica rdf:type owl:Class ;
                       owl:equivalentClass :TopRated ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :Valutazione ;
                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                  owl:onDatatype xsd:float ;
                                                                  owl:withRestrictions ( [ xsd:minExclusive "8.3"^^xsd:float
                                                                                         ]
                                                                                       )
                                                                ]
                                           ] ;
                       rdfs:subClassOf :Critica ;
                       rdfs:comment "I film Acclamati dalla critica sono quei film che hanno ricevuto una valutazione molto positiva da un gruppo di esperti critici" ;
                       rdfs:label "Acclamati dalla critica" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Ambientazione
:Ambientazione rdf:type owl:Class ;
               rdfs:subClassOf :Film ;
               rdfs:comment "Luogo in cui viene ambientato un film" ;
               rdfs:label "Ambientazione" ;
               rdfs:seeAlso <https://dbpedia.org/page/Filming_location> .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Attore
:Attore rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :haRuolo ;
                              owl:someValuesFrom :Attore
                            ] ;
        rdfs:subClassOf :Ruolo ;
        rdfs:comment "Un attore  è chi rappresenta o interpreta una parte o un ruolo in uno spettacolo teatrale, cinematografico, televisivo, radiofonico o di strada." ;
        rdfs:label "Attore" ;
        rdfs:seeAlso <https://dbpedia.org/page/Actor> .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#AttorePopolare
:AttorePopolare rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                             owl:onProperty :RankingAttore ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:int ;
                                                                                  owl:withRestrictions ( [ xsd:minInclusive "1"^^xsd:int
                                                                                                         ]
                                                                                                       )
                                                                                ]
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :RankingAttore ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:int ;
                                                                                  owl:withRestrictions ( [ xsd:maxInclusive "100"^^xsd:int
                                                                                                         ]
                                                                                                       )
                                                                                ]
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :Attore ;
                rdfs:comment "Gli attori popolari sono gli attori più visti dagli utenti" ;
                rdfs:label "AttorePopolare" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#BiancoNero
:BiancoNero rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :AnnoDiUscita ;
                                  owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                       owl:onDatatype xsd:int ;
                                                       owl:withRestrictions ( [ xsd:maxExclusive "1965"^^xsd:int
                                                                              ]
                                                                            )
                                                     ]
                                ] ;
            rdfs:subClassOf :Pellicola ;
            rdfs:comment "vecchia pellicola in bianco e nero, tipica dei film prodotti prima del 1965" ;
            rdfs:label "BiancoNero" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Cast
:Cast rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :haComeCast ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Attore
                          ] ;
      rdfs:subClassOf :Film ;
      rdfs:comment "gruppo di persone che lavora nella produzione di un film" ;
      rdfs:label "Cast" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Colori
:Colori rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :AnnoDiUscita ;
                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                   owl:onDatatype xsd:int ;
                                                   owl:withRestrictions ( [ xsd:minInclusive "1965"^^xsd:int
                                                                          ]
                                                                        )
                                                 ]
                            ] ;
        rdfs:subClassOf :Pellicola ;
        rdfs:comment "pellicola a colori dei film dopo il 1965" ;
        rdfs:label "Colori" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#ConsigliatiDagliUtenti
:ConsigliatiDagliUtenti rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty :Valutazione ;
                                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                   owl:intersectionOf ( [ rdf:type rdfs:Datatype ;
                                                                                          owl:datatypeComplementOf [ rdf:type rdfs:Datatype ;
                                                                                                                     owl:onDatatype xsd:float ;
                                                                                                                     owl:withRestrictions ( [ xsd:minExclusive "8.3"^^xsd:float
                                                                                                                                            ]
                                                                                                                                          )
                                                                                                                   ]
                                                                                        ]
                                                                                        [ rdf:type rdfs:Datatype ;
                                                                                          owl:onDatatype xsd:float ;
                                                                                          owl:withRestrictions ( [ xsd:minInclusive "7.5"^^xsd:float
                                                                                                                 ]
                                                                                                               )
                                                                                        ]
                                                                                      )
                                                                 ]
                                            ] ;
                        rdfs:subClassOf :Popolarità ;
                        rdfs:comment "Film consigliati dalla critica con valutazione almeno 7.5 e non maggiore di 8.3" ;
                        rdfs:label "ConsigliatiDagliUtenti" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Critica
:Critica rdf:type owl:Class ;
         rdfs:label "Critica" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Cult
:Cult rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                          owl:onProperty :AnnoDiUscita ;
                                                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                               owl:onDatatype xsd:int ;
                                                                                               owl:withRestrictions ( [ xsd:minInclusive "1930"^^xsd:int
                                                                                                                      ]
                                                                                                                    )
                                                                                             ]
                                                                        ]
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty :AnnoDiUscita ;
                                                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                               owl:onDatatype xsd:int ;
                                                                                               owl:withRestrictions ( [ xsd:maxInclusive "1999"^^xsd:int
                                                                                                                      ]
                                                                                                                    )
                                                                                             ]
                                                                        ]
                                                                      ) ;
                                                   rdf:type owl:Class
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :Valutazione ;
                                                   owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                        owl:onDatatype xsd:float ;
                                                                        owl:withRestrictions ( [ xsd:minInclusive "8.0"^^xsd:float
                                                                                               ]
                                                                                             )
                                                                      ]
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:subClassOf :Popolarità ;
      rdfs:comment "Film in bianco e nero e a colori che hanno riscontrato grande successo tra la critica e il pubblico, tali da definirsi \"Film di Culto\"" ;
      rdfs:label "Cult" ;
      rdfs:seeAlso <https://dbpedia.org/page/DBCult_Film_Institute> .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#DirettoreAllaMusica
:DirettoreAllaMusica rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :haRuolo ;
                                           owl:someValuesFrom :DirettoreAllaMusica
                                         ] ;
                     rdfs:subClassOf :Ruolo ;
                     rdfs:comment "dirige le musiche del film" ;
                     rdfs:label "DirettoreAllaMusica" ;
                     rdfs:seeAlso <https://dbpedia.org/page/Film_score> .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Drama
:Drama rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :haEventi ;
                             owl:someValuesFrom :Eventi_Drama
                           ] ;
       rdfs:subClassOf :Genere .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Eventi
:Eventi rdf:type owl:Class ;
        rdfs:label "Eventi" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#EventiComici_Divertenti
:EventiComici_Divertenti rdf:type owl:Class ;
                         rdfs:subClassOf :Eventi ;
                         rdfs:comment "eventi divertenti nel film" ;
                         rdfs:label "Eventi comici e divertenti" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#EventiFuturistici
:EventiFuturistici rdf:type owl:Class ;
                   rdfs:subClassOf :Eventi ;
                   rdfs:comment "eventi che potrebbero verificarsi in un futuro prossimo" ;
                   rdfs:label "Eventi Futuristici" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#EventiImmaginari
:EventiImmaginari rdf:type owl:Class ;
                  rdfs:subClassOf :Eventi ;
                  rdfs:comment "eventi che sono frutto della fantasia" ;
                  rdfs:label "Eventi Immaginari" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#EventiStorici
:EventiStorici rdf:type owl:Class ;
               rdfs:subClassOf :Eventi ;
               rdfs:comment "eventi avvenuti nel passato" ;
               rdfs:label "Eventi Storici" .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Eventi_Drama
:Eventi_Drama rdf:type owl:Class ;
              rdfs:subClassOf :Eventi .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#Film
:Film rdf:type owl:Class ;
      rdfs:label "Film" ;
      rdfs:seeAlso <https://dbpedia.org/page/Film> .


###  http://www.semanticweb.org/mario/ontologies/esame/progettoCinema#FilmAmericano
