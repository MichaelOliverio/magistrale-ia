#################################################################
#    Rules
#################################################################

:b rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:n rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:m rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:nm rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:g rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:birra rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:birrificio rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Mapping fra la classe Birra e la classe Beer di Bevon."^^xsd:string ;
   rdfs:label "Birra in Bevon"^^xsd:string ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#classPredicate> <http://dbpedia.org/ontology/Beer> ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :b
                                                     ] ;
                                           rdf:rest rdf:nil
                                         ] ;
   <http://www.w3.org/2003/11/swrl#head> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#classPredicate> <http://rdfs.co/bevon/Beer> ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :b
                                                     ] ;
                                           rdf:rest rdf:nil
                                         ]
 ] .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Categorizza una birra come BirraOmonima se il suo nome contiene quello del birrificio."^^xsd:string ;
   rdfs:label "BirraOmonima"^^xsd:string ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
