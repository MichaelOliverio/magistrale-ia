[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Birra
                :Birrificio
                :Caratteristica
                :Colore
                :Marca
                :Stile
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :BirraAmbrata
                :BirraChiara
                :BirraScura
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :ColoreAmbrato
                :ColoreChiaro
                :ColoreScuro
              )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :AbbaziaDiAchel
                        :AbbaziaDiChimay
                        :AbbaziaDiOrval
                        :AbbaziaDiRochefort
                        :AbbaziaDiWestmalle
                        :AbbaziaDiWestvleteren
                      )
] .


