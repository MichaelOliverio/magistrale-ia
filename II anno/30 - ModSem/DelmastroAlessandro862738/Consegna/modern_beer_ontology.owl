@prefix : <http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix viaf: <http://viaf.org/viaf/> .
@prefix bevon: <http://rdfs.co/bevon/> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix wikidata: <http://www.wikidata.org/wiki/> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@prefix modern_beer_ontology: <http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#> .
@base <http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology> .

<http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology> rdf:type owl:Ontology ;
                                                                         owl:imports <http://www.ontologydesignpatterns.org/cp/owl/classification.owl> ,
                                                                                     <http://www.ontologydesignpatterns.org/cp/owl/set.owl> ;
                                                                         dc:creator "Alessandro Delmastro"@it ;
                                                                         dc:date "10/08/2021"@it ;
                                                                         dc:description "L'ontologia rappresenta le birre sulla base di caratteristiche percepibili al consumatore e a tutti coloro che non hanno competenze specifiche. A partire da informazioni facilmente reperibili sulla confezione del prodotto, le birre vengono categorizzate rispetto alle macrocategorie di: gusto, colore e contesto."@it ;
                                                                         dc:title "Modern Beer Ontology"@it ;
                                                                         vann:preferredNamespacePrefix "mbe"@it ;
                                                                         vann:preferredNamespaceUri "http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology"@it ;
                                                                         rdfs:label "Modern Beer Ontology"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#equivalentClass
owl:equivalentClass rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#narrowMatch
skos:narrowMatch rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://rdfs.co/bevon/brewery
bevon:brewery rdf:type owl:ObjectProperty ;
              rdfs:comment "Indica il birrificio che produce una birra in Bevon."@it ;
              rdfs:label "brewery"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#caratterizzatoDa
modern_beer_ontology:caratterizzatoDa rdf:type owl:ObjectProperty ;
                                      rdfs:domain modern_beer_ontology:Stile ;
                                      rdfs:range modern_beer_ontology:Caratteristica ;
                                      rdfs:comment "Caratteristica tipica dello stile."@it ;
                                      rdfs:label "caratterizzato da"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#commercializza
modern_beer_ontology:commercializza rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                                    owl:inverseOf modern_beer_ontology:haMarca ;
                                    rdfs:domain modern_beer_ontology:Marca ;
                                    rdfs:range modern_beer_ontology:Birra ;
                                    rdfs:comment "Indica che una marca commercializza una determinata birra."@it ;
                                    rdfs:label "commercializza"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#haCaratteristicaBirra
modern_beer_ontology:haCaratteristicaBirra rdf:type owl:ObjectProperty ;
                                           rdfs:domain modern_beer_ontology:Birra ;
                                           rdfs:range modern_beer_ontology:Caratteristica ;
                                           rdfs:comment "Caratteristica peculiare della birra, non è tipica dello stile."@it ;
                                           rdfs:label "ha caratteristica birra"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#haCaratteristicaStile
modern_beer_ontology:haCaratteristicaStile rdf:type owl:ObjectProperty ;
                                           rdfs:domain modern_beer_ontology:Birra ;
                                           rdfs:range modern_beer_ontology:Caratteristica ;
                                           owl:propertyChainAxiom ( modern_beer_ontology:haStile
                                                                    modern_beer_ontology:caratterizzatoDa
                                                                  ) ;
                                           rdfs:comment "Caratteristica di una birra propria dello stile a cui appartiene."@it ;
                                           rdfs:label "ha caratteristica stile"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#haColore
modern_beer_ontology:haColore rdf:type owl:ObjectProperty ;
                              rdfs:domain modern_beer_ontology:Birra ;
                              rdfs:range modern_beer_ontology:Colore ;
                              rdfs:comment "Indica il colore di una birra."@it ;
                              rdfs:label "ha colore"@it ;
                              skos:exactMatch <https://dbpedia.org/ontology/colour> .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#haMarca
modern_beer_ontology:haMarca rdf:type owl:ObjectProperty ,
                                      owl:FunctionalProperty ;
                             rdfs:domain modern_beer_ontology:Birra ;
                             rdfs:range modern_beer_ontology:Marca ;
                             rdfs:comment "Indica la marca che commercializza la birra."@it ;
                             rdfs:label "ha marca"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#haStile
modern_beer_ontology:haStile rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#isClassifiedBy> ;
                             rdfs:domain modern_beer_ontology:Birra ;
                             rdfs:range modern_beer_ontology:Stile ;
                             rdfs:comment "Indica lo stile di produzione di una birra."@it ;
                             rdfs:label "ha stile"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#ispirataA
modern_beer_ontology:ispirataA rdf:type owl:ObjectProperty ,
                                        owl:TransitiveProperty ;
                               rdfs:domain modern_beer_ontology:Birra ;
                               rdfs:range modern_beer_ontology:Birra ;
                               rdfs:comment "La birra è ispirata ad un'altra birra."@it ;
                               rdfs:label "ispirata a"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#possedutaDa
modern_beer_ontology:possedutaDa rdf:type owl:ObjectProperty ;
                                 owl:inverseOf modern_beer_ontology:possiede ;
                                 rdf:type owl:FunctionalProperty ;
                                 rdfs:domain modern_beer_ontology:Marca ;
                                 rdfs:range modern_beer_ontology:Birrificio ;
                                 rdfs:comment "Indica che la marca è di proprietà di un produttore."@it ;
                                 rdfs:label "posseduta da"@it ;
                                 skos:exactMatch <https://dbpedia.org/ontology/owner> .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#possiede
modern_beer_ontology:possiede rdf:type owl:ObjectProperty ;
                              rdfs:domain modern_beer_ontology:Birrificio ;
                              rdfs:range modern_beer_ontology:Marca ;
                              rdfs:comment "Indica che un produttore possiede una marca."@it ;
                              rdfs:label "possiede"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#prodottaDa
modern_beer_ontology:prodottaDa rdf:type owl:ObjectProperty ;
                                owl:inverseOf modern_beer_ontology:produce ;
                                rdfs:domain modern_beer_ontology:Birra ;
                                rdfs:range modern_beer_ontology:Birrificio ;
                                owl:propertyChainAxiom ( modern_beer_ontology:haMarca
                                                         modern_beer_ontology:possedutaDa
                                                       ) ;
                                rdfs:comment "La birra è prodotta da un produttore, che possiede la marca che la commercializza."@it ;
                                rdfs:label "prodotta da"@it ;
                                skos:exactMatch <https://dbpedia.org/ontology/manufacturer> .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#produce
modern_beer_ontology:produce rdf:type owl:ObjectProperty ;
                             rdfs:domain modern_beer_ontology:Birrificio ;
                             rdfs:range modern_beer_ontology:Birra ;
                             rdfs:comment "Un birrificio produce una determinata birra."@it ;
                             rdfs:label "produce"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#IBU
modern_beer_ontology:IBU rdf:type owl:DatatypeProperty ;
                         rdfs:domain modern_beer_ontology:Birra ;
                         rdfs:range xsd:unsignedInt ;
                         rdfs:comment "Indice di amarezza di una bevanda."@it ;
                         rdfs:label "IBU"@it ;
                         skos:exactMatch bevon:ibu .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#NomeMarca
modern_beer_ontology:NomeMarca rdf:type owl:DatatypeProperty ;
                               rdfs:domain modern_beer_ontology:Marca ;
                               rdfs:range xsd:string ;
                               rdfs:comment "Nome della marca."@it ;
                               rdfs:label "Nome marca"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#gradazioneAlcolica
modern_beer_ontology:gradazioneAlcolica rdf:type owl:DatatypeProperty ;
                                        rdfs:domain modern_beer_ontology:Birra ;
                                        rdfs:range xsd:float ;
                                        rdfs:comment "Quantità di alcol presente nella bevanda."@it ;
                                        rdfs:label "Gradazione alcolica"@it ;
                                        skos:exactMatch bevon:abv .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#nome
modern_beer_ontology:nome rdf:type owl:DatatypeProperty ;
                          rdfs:domain modern_beer_ontology:Birra ;
                          rdfs:range xsd:string ;
                          rdfs:comment "Nome della birra."@it ;
                          rdfs:label "Nome"@it ;
                          skos:narrowMatch bevon:name .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#temperaturaDiServizio
modern_beer_ontology:temperaturaDiServizio rdf:type owl:DatatypeProperty ;
                                           rdfs:domain modern_beer_ontology:Birra ;
                                           rdfs:range xsd:integer ;
                                           rdfs:comment "Temperatura consigliata a cui bere la birra."@it ;
                                           rdfs:label "Temperatura di servizio"@it ;
                                           skos:narrowMatch "https://dbpedia.org/page/Temperature"@it .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Beer
dbo:Beer rdf:type owl:Class ;
         owl:equivalentClass modern_beer_ontology:Birra ;
         rdfs:comment "Classe Birra in dbpedia."@it ;
         rdfs:label "Birra"@it .


###  http://dbpedia.org/ontology/Brand
dbo:Brand rdf:type owl:Class ;
          owl:equivalentClass modern_beer_ontology:Marca ;
          rdfs:comment "Classe Marca in dbpedia."@it ;
          rdfs:label "Marca"@it .


###  http://dbpedia.org/ontology/Brewery
dbo:Brewery rdf:type owl:Class ;
            owl:equivalentClass modern_beer_ontology:Birrificio ;
            rdfs:comment "Classe birrificio in dbpedia."@it ;
            rdfs:label "Birrificio"@it .


###  http://dbpedia.org/ontology/Colour
dbo:Colour rdf:type owl:Class ;
           owl:equivalentClass modern_beer_ontology:Colore ;
           rdfs:comment "Classe Colore in dbpedia."@it ;
           rdfs:label "Colore"@it .


###  http://rdfs.co/bevon/Beer
bevon:Beer rdf:type owl:Class ;
           rdfs:comment "Classe Beer in Bevon."@it ;
           rdfs:label "BeerBevon"@en .


###  http://rdfs.co/bevon/Brewery
bevon:Brewery rdf:type owl:Class ;
              rdfs:comment "Classe Birrificio in Bevon."@it ;
              rdfs:label "Brewery"@en .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#Abbaziale
modern_beer_ontology:Abbaziale rdf:type owl:Class ;
                               rdfs:subClassOf modern_beer_ontology:Ale ;
                               rdfs:comment "Stile Ale ispirato alla tradizione dei monasteri trappisti."@it ;
                               rdfs:label "Abbaziale"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#Ale
modern_beer_ontology:Ale rdf:type owl:Class ;
                         rdfs:subClassOf modern_beer_ontology:Stile ;
                         owl:disjointWith modern_beer_ontology:Lager ;
                         rdfs:comment "Ale è una macrocategoria di stili di birra che si caratterizza per l'utilizzo di lieviti la cui fermentazione avviene ad alte temperature."@it ;
                         rdfs:label "Ale"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#Birra
modern_beer_ontology:Birra rdf:type owl:Class ;
                           owl:equivalentClass <https://www.wikidata.org/wiki/Q44> ;
                           rdfs:comment "Bevanda alcolica ottenuta dalla fermentazione di mosto a base di malto d'orzo e luppolo."@it ;
                           rdfs:label "Birra"@it ;
                           skos:exactMatch <http://zbw.eu/stw/descriptor/14973-2> .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraAlcolica
modern_beer_ontology:BirraAlcolica rdf:type owl:Class ;
                                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                                         owl:onProperty modern_beer_ontology:gradazioneAlcolica ;
                                                         owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                              owl:onDatatype xsd:float ;
                                                                              owl:withRestrictions ( [ xsd:minExclusive "7.0"^^xsd:float
                                                                                                     ]
                                                                                                   )
                                                                            ]
                                                       ] ;
                                   rdfs:subClassOf modern_beer_ontology:Birra ;
                                   owl:disjointWith modern_beer_ontology:BirraLeggera ;
                                   rdfs:comment "Birra caratterizzata da una forte componente alcolica, superiore al 7%."@it ;
                                   rdfs:label "Birra alcolica"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraAmara
modern_beer_ontology:BirraAmara rdf:type owl:Class ;
                                owl:equivalentClass [ rdf:type owl:Restriction ;
                                                      owl:onProperty modern_beer_ontology:IBU ;
                                                      owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                           owl:onDatatype xsd:positiveInteger ;
                                                                           owl:withRestrictions ( [ xsd:minInclusive "40"^^xsd:positiveInteger
                                                                                                  ]
                                                                                                )
                                                                         ]
                                                    ] ;
                                rdfs:subClassOf modern_beer_ontology:Birra ;
                                owl:disjointWith modern_beer_ontology:BirraDolce ;
                                rdfs:comment "Birra caratterizzata da un sapore amaro, con IBU non inferiore a 40."@it ;
                                rdfs:label "Birra amara"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraAmbrata
modern_beer_ontology:BirraAmbrata rdf:type owl:Class ;
                                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                                        owl:onProperty modern_beer_ontology:haColore ;
                                                        owl:someValuesFrom modern_beer_ontology:ColoreAmbrato
                                                      ] ;
                                  rdfs:subClassOf modern_beer_ontology:Birra ;
                                  rdfs:comment "Birra dal colore ambrato."@it ;
                                  rdfs:label "Birra ambrata"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraChiara
modern_beer_ontology:BirraChiara rdf:type owl:Class ;
                                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                                       owl:onProperty modern_beer_ontology:haColore ;
                                                       owl:someValuesFrom modern_beer_ontology:ColoreChiaro
                                                     ] ;
                                 rdfs:subClassOf modern_beer_ontology:Birra ;
                                 rdfs:comment "Birra dal colore chiaro, bianca o bionda."@it ;
                                 rdfs:label "Birra chiara"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraDaCarne
modern_beer_ontology:BirraDaCarne rdf:type owl:Class ;
                                  owl:equivalentClass [ owl:intersectionOf ( modern_beer_ontology:BirraAlcolica
                                                                             modern_beer_ontology:BirraDaPasto
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ] ;
                                  rdfs:subClassOf modern_beer_ontology:BirraDaPasto ;
                                  rdfs:comment "Birra da pasto particolarmente indicata per la carne."@it ;
                                  rdfs:label "Birra da carne"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraDaContesto
modern_beer_ontology:BirraDaContesto rdf:type owl:Class ;
                                     rdfs:subClassOf modern_beer_ontology:Birra ;
                                     rdfs:comment "Birra che ha uno più contesti nei quali rende al meglio."@it ;
                                     rdfs:label "Birra da contesto"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraDaDivano
modern_beer_ontology:BirraDaDivano rdf:type owl:Class ;
                                   owl:equivalentClass [ owl:intersectionOf ( modern_beer_ontology:BirraAlcolica
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty modern_beer_ontology:temperaturaDiServizio ;
                                                                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                     owl:onDatatype xsd:integer ;
                                                                                                     owl:withRestrictions ( [ xsd:minExclusive 6
                                                                                                                            ]
                                                                                                                          )
                                                                                                   ]
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ] ;
                                   rdfs:subClassOf modern_beer_ontology:BirraDaContesto ;
                                   rdfs:comment "Birra dalla alta gradazione alcolica, si presta ad essere bevuta in quantità limitata in contesti conviviali."@it ;
                                   rdfs:label "Birra da divano"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraDaPasto
modern_beer_ontology:BirraDaPasto rdf:type owl:Class ;
                                  owl:equivalentClass [ owl:intersectionOf ( modern_beer_ontology:Birra
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty modern_beer_ontology:gradazioneAlcolica ;
                                                                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                    owl:onDatatype xsd:float ;
                                                                                                    owl:withRestrictions ( [ xsd:maxInclusive "9.0"^^xsd:float
                                                                                                                           ]
                                                                                                                         )
                                                                                                  ]
                                                                             ]
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty modern_beer_ontology:temperaturaDiServizio ;
                                                                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                    owl:onDatatype xsd:integer ;
                                                                                                    owl:withRestrictions ( [ xsd:minExclusive 2
                                                                                                                           ]
                                                                                                                         )
                                                                                                  ]
                                                                             ]
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ] ;
                                  rdfs:subClassOf modern_beer_ontology:BirraDaContesto ;
                                  rdfs:comment "Birra adatta ad essere consumata durante i pasti."@it ;
                                  rdfs:label "Birra da pasto"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraDaPizza
modern_beer_ontology:BirraDaPizza rdf:type owl:Class ;
                                  owl:equivalentClass [ owl:intersectionOf ( modern_beer_ontology:BirraDaPasto
                                                                             modern_beer_ontology:BirraRinfrescante
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ] ;
                                  rdfs:subClassOf modern_beer_ontology:BirraDaPasto ;
                                  rdfs:comment "Birra da pasto particolarmente indicata per la pizza."@it ;
                                  rdfs:label "Birra da pizza"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraDolce
modern_beer_ontology:BirraDolce rdf:type owl:Class ;
                                owl:equivalentClass [ rdf:type owl:Restriction ;
                                                      owl:onProperty modern_beer_ontology:IBU ;
                                                      owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                           owl:onDatatype xsd:positiveInteger ;
                                                                           owl:withRestrictions ( [ xsd:maxExclusive "40"^^xsd:positiveInteger
                                                                                                  ]
                                                                                                )
                                                                         ]
                                                    ] ;
                                rdfs:subClassOf modern_beer_ontology:Birra ;
                                rdfs:comment "Birra dal gusto dolce, con IBU inferiore a 35."@it ;
                                rdfs:label "Birra dolce"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraLeggera
modern_beer_ontology:BirraLeggera rdf:type owl:Class ;
                                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                                        owl:onProperty modern_beer_ontology:gradazioneAlcolica ;
                                                        owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                             owl:onDatatype xsd:float ;
                                                                             owl:withRestrictions ( [ xsd:maxInclusive "5.0"^^xsd:float
                                                                                                    ]
                                                                                                  )
                                                                           ]
                                                      ] ;
                                  rdfs:subClassOf modern_beer_ontology:Birra ;
                                  rdfs:comment "Birra dalla bassa gradazione alcolica, inferiore al 4%."@it ;
                                  rdfs:label "Birra leggera"@it .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraOmonima
modern_beer_ontology:BirraOmonima rdf:type owl:Class ;
                                  rdfs:subClassOf modern_beer_ontology:Birra .


###  http://www.semanticweb.org/alessandro/ontologies/modern_beer_ontology#BirraRicercata
modern_beer_ontology:BirraRicercata rdf:type owl:Class ;
                                    owl:equivalentClass [ owl:intersectionOf ( modern_beer_ontology:Birra
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty modern_beer_ontology:haCaratteristicaBirra ;
                                                                                 owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                                                 owl:onClass modern_beer_ontology:Caratteristica
                                                                               ]
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ] ;
